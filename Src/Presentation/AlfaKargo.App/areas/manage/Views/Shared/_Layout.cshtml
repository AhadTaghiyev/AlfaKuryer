    <script>$("body").on("change", ".imginput", function (event) {
            $($(this).parent("div").children(".editedImage")).attr("src", URL.createObjectURL(event.target.files[0]));
            $($(this).parent("div").children(".editedImage")).css("display", "inline-block");
        })
        $(".wrapper input").change(function () {
            $(".wrapper .newimage").remove();
            for (var i = 0; i < event.target.files.length; i++) {
                $(".wrapper").append(
                    '<img class="m-1 newimage" src="' + URL.createObjectURL(event.target.files[i]) + '"   width="100" height="100" alt="Alternate Text" />'
                )
            }
        })
        $(".wrapper2 input").change(function () {
            $(".wrapper2 .newimage2").remove();
            for (var i = 0; i < event.target.files.length; i++) {
                $(".wrapper2").append(
                    '<img class="m-1 newimage2" src="' + URL.createObjectURL(event.target.files[i]) + '"   width="100" height="100" alt="Alternate Text" />'
                )
            }
        })</script>

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <script type="text/javascript">$(function () {
            var table = document.querySelector("table");
            if ($(table).attr("data-emrah") != null) {
                let tableName = $(table).attr("data-emrah");
                let tBody = $(table).children("tbody");
                $(tBody).children("tr").css("cursor", "row-resize");
                $(tBody).sortable();
                $(".text-center.mb-3.mt-3").append("<button type='button' class='btn  btn-success  confirmqueue'><i class='fas fa-check mr-1'></i>Sıralamanı təsdiqlə</button>");
                $("body").on("click", ".confirmqueue", function () {
                    var orderArray = [];
                    for (var i = 0; i < $(tBody).children("tr").length; i++) {
                        orderArray.push(new row($(tBody).children("tr")[i].getAttribute("data-id"), i));
                    }
                    $.ajax({
                        url: "/admin/ajax/confirmqueue/",
                        type: "post",
                        data: { order: orderArray, classname: tableName },
                        datatype: "json",
                        success: function (res) {
                            if (res.status == 200) {
                                swal("Sıralama qeydə alındı.Təşəkkürlər", "", "success");
                                setTimeout(function () {
                                    window.location.reload();
                                }, 1500)
                            }
                        }
                    })
                    function row(id, place) {
                        this.Id = id,
                            this.Place = place
                    }
                })
            }
        })
        $('.myTable').DataTable({
            "order": [[0, "desc"]],
            "language": {
                "sDecimal": ",",
                "sEmptyTable": "Sistemdə heç bir məlumat yoxdur",
                "sInfo": "_TOTAL_ məlumatdan _START_ - _END_ arasındakı məlumatlar göstərilir",
                "sInfoEmpty": "Məlumat yoxdur",
                "sInfoFiltered": "(_MAX_ məlumat içindən tapılan)",
                "sInfoPostFix": "",
                "sInfoThousands": ".",
                "sLengthMenu": "Səhifədə _MENU_ məlumat göstər",
                "sLoadingRecords": "Yüklənir...",
                "sProcessing": "İşlənir...",
                "sSearch": "Axtar:",
                "sZeroRecords": "Sorğuya əsasən məlumat yoxdur",
